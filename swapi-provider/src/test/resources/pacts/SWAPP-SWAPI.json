{
    "provider": {
        "name": "SWAPI"
    },
    "consumer": {
        "name": "SWAPP"
    },
    "interactions": [
        {
            "description": "a request for characters'",
            "request": {
                "method": "GET",
                "path": "/people"
            },
            "response": {
                "status": 200,
                "body": [
                    {
                        "gender": "male",
                        "id": 1,
                        "name": "Luke Skywalker"
                    },
                    {
                        "gender": "n/a",
                        "id": 2,
                        "name": "C-3PO"
                    }
                ]
            },
            "providerStates": [
                {
                    "name": "star wars data exist"
                }
            ]
        },
        {
            "description": "a request for person with ID 1'",
            "request": {
                "method": "GET",
                "path": "/people/1"
            },
            "response": {
                "status": 200,
                "body": {
                    "gender": "male",
                    "id": 1,
                    "name": "Luke Skywalker"
                }
            },
            "providerStates": [
                {
                    "name": "star wars data exist"
                }
            ]
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.16"
        }
    }
}